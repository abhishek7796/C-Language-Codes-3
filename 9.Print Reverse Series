#include<stdio.h>

/*
Reverse Series
By:AYS
*/

int main()
{
	int n,rev=0,rem;
	printf("\n*****Reversed Series*****\n");
	printf("\n\nEnter a Number:-\n\n");
	scanf("%d",&n);	

    while(n!=0)
    {
    	rem=n%10;
    	rev=rev*10+rem;
    	n=n/10;
   	}
	printf("\nThe Reversed Series is:- %d\n",rev);
	return 0;	
}
